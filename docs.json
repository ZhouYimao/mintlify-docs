{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Cagen",
  "colors": {
    "primary": "#000000",
    "light": "#ffffff",
    "dark": "#1d1d1f"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "customCSS": ["emagen-theme.css"],
  "favicon": "/favicon-square.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "changelog"
            ]
          },
          {
            "group": "Tutorials",
            "icon": "graduation-cap",
            "pages": [
              {
                "page": "Getting Started",
                "pages": [
                  "tutorials/getting-started/first-agent",
                  "tutorials/getting-started/first-workflow"
                ]
              },
              {
                "page": "Use Cases",
                "pages": [
                  "tutorials/use-cases/startup-guide"
                ]
              }
            ]
          },
          {
            "group": "Concepts",
            "icon": "lightbulb",
            "pages": [
              "explanation/core-concepts/ai-native-organization"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord Community",
          "href": "https://discord.gg/cagen",
          "icon": "discord"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/emagen-ai",
          "icon": "github"
        },
        {
          "anchor": "Product Hunt",
          "href": "https://producthunt.com/products/cagen",
          "icon": "rocket"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/cagen-full.svg",
    "dark": "/logo/cagen-full.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@emagen.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch Cagen",
      "href": "https://app.cagen.ai"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/emagenai",
      "github": "https://github.com/emagen-ai",
      "linkedin": "https://www.linkedin.com/company/emagen-ai"
    }
  }
}