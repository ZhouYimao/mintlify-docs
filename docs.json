{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Cagen",
  "theme": "mint",
  "logo": {
    "dark": "/logo/cagen-full.svg",
    "light": "/logo/cagen-full.svg"
  },
  "favicon": "/favicon-square.svg",
  "colors": {
    "primary": "#000000",
    "light": "#ffffff",
    "dark": "#1d1d1f"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "customCSS": ["emagen-theme.css"],
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:support@emagen.ai"
    }
  ],
  "topbarCtaButton": {
    "name": "Launch Cagen",
    "url": "https://app.cagen.ai"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Welcome",
        "groups": [
          {
            "group": "Home",
            "pages": [
              "index"
            ]
          },
          {
            "group": "Getting Started",
            "pages": [
              "get-started",
              "quickstart",
              "use-cases",
              "success-stories",
              "transformation-matrix"
            ]
          },
          {
            "group": "What's New",
            "pages": [
              "changelog"
            ]
          }
        ]
      },
      {
        "tab": "Paths",
        "groups": [
          {
            "group": "Choose Your Path",
            "pages": [
              "paths/startup-founder",
              "paths/solopreneur",
              "paths/small-business",
              "paths/enterprise",
              "paths/developer",
              "paths/agency"
            ]
          }
        ]
      },
      {
        "tab": "Solutions",
        "groups": [
          {
            "group": "By Department",
            "pages": [
              "use-cases/engineering",
              "use-cases/customer-support",
              "use-cases/sales",
              "use-cases/marketing",
              "use-cases/operations",
              "use-cases/product-management"
            ]
          }
        ]
      },
      {
        "tab": "How-to Guides",
        "groups": [
          {
            "group": "Work Management",
            "pages": [
              "how-to/work/issue-management"
            ]
          },
          {
            "group": "Memory Bank",
            "pages": [
              "how-to/memory/manage-memory-bank"
            ]
          },
          {
            "group": "Actions",
            "pages": [
              "how-to/actions/create-action"
            ]
          },
          {
            "group": "MCP Integration",
            "pages": [
              "how-to/mcp/mcp-integration"
            ]
          },
          {
            "group": "Team Management",
            "pages": [
              "how-to/members/team-management"
            ]
          },
          {
            "group": "Agents",
            "pages": [
              "how-to/agents/create-custom-agent"
            ]
          }
        ]
      },
      {
        "tab": "Concepts",
        "groups": [
          {
            "group": "Core Concepts",
            "pages": [
              "explanation/core-concepts/ai-native-organization",
              "explanation/core-concepts/cagen-system-agent",
              "explanation/concepts/ai-collaboration",
              "explanation/concepts/memory-intelligence"
            ]
          },
          {
            "group": "System Architecture",
            "pages": [
              "explanation/architecture/system-design"
            ]
          },
          {
            "group": "Best Practices",
            "pages": []
          }
        ]
      },
      {
        "tab": "Reference",
        "groups": [
          {
            "group": "Interface",
            "pages": [
              "reference/interface/command-launcher",
              "reference/interface/inbox"
            ]
          },
          {
            "group": "Communication",
            "pages": [
              "reference/communication/conversations"
            ]
          },
          {
            "group": "System",
            "pages": [
              "reference/system/runtime-environment"
            ]
          },
          {
            "group": "Agent Types",
            "pages": [
              "reference/agent-types/built-in-agents"
            ]
          },
          {
            "group": "General",
            "pages": [
              "reference/pricing-plans"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://discord.gg/cagen",
          "icon": "discord"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/emagen-ai",
          "icon": "github"
        },
        {
          "anchor": "Product Hunt",
          "href": "https://producthunt.com/products/cagen",
          "icon": "rocket"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@emagen.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch Cagen",
      "href": "https://app.cagen.ai"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/emagen_ai",
      "github": "https://github.com/emagen-ai",
      "linkedin": "https://www.linkedin.com/company/emagen-ai"
    }
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      {
        "title": "Ask Cagen",
        "description": "Ask Cagen AI about this documentation",
        "icon": "robot",
        "href": {
          "base": "https://app.cagen.ai/public/chat",
          "query": [
            {
              "key": "context",
              "value": "$page"
            },
            {
              "key": "source",
              "value": "docs"
            }
          ]
        }
      }
    ]
  }
}